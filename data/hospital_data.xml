<!--in this file pre selected data will be showing here so no user have to re-enter basic info -->

<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="hospital" model="hospital.doctor">
            <field name="Doctor_name">Name</field>
            <field name="">check_printing</field>
            <field name="payment_type">outbound</field>
            <field name="sequence">-100</field>
        </record>

        <function model="account.journal" name="_enable_check_printing_on_bank_journals"/>


        <record model="ir.actions.server" id="action_account_print_checks">
            <field name="name">Print Checks</field>
            <field name="model_id" ref="account.model_account_payment"/>
            <field name="binding_model_id" ref="account.model_account_payment" />
            <field name="binding_view_types">list</field>
            <field name="groups_id" eval="[(4, ref('account.group_account_user'))]"/>
            <field name="state">code</field>
            <field name="code">
if records: 
    action = records.print_checks()
            </field>
        </record>

    </data>
</odoo>
